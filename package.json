    "@aws-sdk/client-rds-data": "^3.835.0",
    "@aws-sdk/client-s3": "^3.837.0",
    "@aws-sdk/client-secrets-manager": "^3.835.0",
    "aws-sdk": "^2.1692.0",
    "next": "15.3.4",
    "pg": "^8.11.3",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^20",
    "@types/pg": "^8.10.9",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.3.4",
    "jest": "^30.0.3",
