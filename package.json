{
  "name": "pickem-v2",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "db:migrate": "node scripts/run-migrations.js",
    "db:seed": "node scripts/run-migrations.js && node database/seeds/run-seeds.js",
